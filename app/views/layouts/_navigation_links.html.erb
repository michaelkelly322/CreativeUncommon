<%# add navigation links to this file %>
<%= link_to 'Home', root_path, class: 'nav-tile' %>
<%= link_to 'User Guide', guide_path, class: 'nav-tile' %>
<%= link_to 'FAQ', faq_path, class: 'nav-tile' %>
<%= link_to 'About Us', about_path, class: ' nav-tile' %>

<% if !signed_in? %>
	<%= link_to 'Login', signin_path, class: ' nav-tile' %>
	<%= link_to 'Signup', signup_path, class: 'nav-tile' %>
<% else %>
	<ul>
		<li id="user-menu" class="dropdown">
			<a href='#' class="dropdown-toggle" data-toggle="dropdown">
				Account<b class="caret"></b>
			</a>
			<ul class="dropdown-menu">
				<li><%= link_to 'Profile', current_user %></li>
				<li><%= link_to 'Account Settings', '#' %></li>
				<li class="divider"></li>
				<li><%= link_to 'Logout', '#' %></li>
			</ul>
		</li>
	</ul>
<% end %>
